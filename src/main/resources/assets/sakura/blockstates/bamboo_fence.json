{
    "multipart": [
        {   "apply": { "model": "sakura:bamboo" },
			"when": { "up": "true" }
		},
        {   "when": { "north": "true" },
            "apply": { "model": "sakura:bamboo_fence", "uvlock": false }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "sakura:bamboo_fence", "y": 90, "uvlock": false }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "sakura:bamboo_fence", "y": 180, "uvlock": false }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "sakura:bamboo_fence", "y": 270, "uvlock": false }
        }
    ]
}
